// This an example of how now to do things.
{
  tokens = [
      PLUS = '+'
      MINUS = '-'
      MUL = '*'
      DIV = '/'
      OPEN_B = '('
      CLOSE_B = ')'
      NUM="regexp:\d+"
    ]
}

calcFile ::= item_*

private item_ ::= expr | CRLF

expr ::=  term '+' expr |
          term '-' expr |
          term
term ::= factor '*' term |
         factor '/' term |
         factor
factor ::= NUM | OPEN_B expr CLOSE_B